    "System.Linq": "4.0.1-beta-23516",
    "System.Threading": "4.0.11-beta-23516",
    "WebVella.ERP": "1.0.0-*"

  },

	"commands": {
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel",
		"temp-web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://erp.com"
	},

  "frameworks": {
    "dnx46": {
      "frameworkAssemblies": {
        "System.Drawing": "4.0.0.0"
      }
    }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
